@Library('libraries')_

pipeline
{
    agent any
    stages
    {
    stage('continous download')
    {
        steps
        {
         script
         {
         cicd.newgit("https://github.com/lalitha419/new-maven.git")   
         }
        }
    }
     stage('continous build')
    {
        steps
        {
         script
         {
         cicd.newmaven() 
         }
        }
    }
   }
 }  
